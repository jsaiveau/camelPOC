<?xml version="1.0" encoding="UTF-8"?>

  <routes xmlns="http://camel.apache.org/schema/spring">
  <route>
    <from uri="direct:updateRoutes"/>
    <bean ref="routeService" method="updateRoutes"/>
  </route>
  <route>
    <from uri="direct:getRoutes"/>
    <bean ref="routeService" method="getRoutes"/>
  </route>
</routes>

